{
  "name": "ember-fastboot-app-tests",
  "version": "0.3.1",
  "description": "FastBoot testing support for Ember apps",
  "keywords": [
    "ember-addon",
    "fastboot",
    "tests"
  ],
  "license": "MIT",
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-fastboot-app-tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests node-tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "nodetest": "mocha node-tests --recursive",
    "fastboottest": "DEBUG=ember-fastboot-addon-tests,ember-cli-addon-tests ember fastboot:test"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "core-object": "^3.1.5",
    "debug": "^3.1.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-babel": "^6.8.2",
    "jquery": "^3.3.1",
    "jsdom": "^11.10.0",
    "mocha": "^5.1.1",
    "request": "^2.85.0",
    "rsvp": "^4.7.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "ember-cli": "~3.1.4",
    "ember-cli-app-version": "^3.1.0",
    "ember-cli-blueprint-test-helpers": "^0.19.0",
    "ember-cli-dependency-checker": "^2.1.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-fastboot": "^2.0.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^8.0.1",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "6.* || >= 8.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
